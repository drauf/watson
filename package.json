{
  "name": "watson",
  "version": "0.1.0",
  "description": "Watson is a web-based, offline Java thread dump analyzer",
  "author": "Daniel Rauf <danielrauf@gmail.com> (https://github.com/drauf/)",
  "license": "MIT",
  "homepage": "https://drauf.github.io/watson/",
  "repository": {
    "type": "git",
    "url": "https://github.com/drauf/watson.git"
  },
  "bugs": {
    "url": "https://github.com/drauf/watson/issues"
  },
  "dependencies": {
    "d3": "7.8.4",
    "d3-flame-graph": "4.1.3",
    "flatted": "3.2.7",
    "gh-pages": "5.0.0",
    "localforage": "1.10.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-router-dom": "6.11.2",
    "recharts": "2.6.2",
    "spark-md5": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.22.1",
    "@playwright/test": "1.34.3",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@svgr/webpack": "8.0.1",
    "@types/d3": "7.4.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.2.5",
    "@types/react-dom": "18.2.4",
    "@types/react-router-dom": "5.3.3",
    "@types/spark-md5": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "autoprefixer": "10.4.14",
    "babel-jest": "29.5.0",
    "babel-loader": "9.1.2",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "bfj": "7.0.2",
    "browserslist": "4.21.7",
    "camelcase": "7.0.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "5.0.0",
    "dotenv": "16.1.3",
    "dotenv-expand": "10.0.0",
    "eslint": "8.41.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "fs-extra": "11.1.1",
    "html-webpack-plugin": "5.5.1",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-resolve": "29.5.0",
    "jest-watch-typeahead": "2.2.2",
    "mini-css-extract-plugin": "2.7.6",
    "postcss": "8.4.24",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "7.3.2",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "8.4.1",
    "prompts": "2.4.2",
    "react-dev-utils": "12.0.1",
    "react-refresh": "0.14.0",
    "resolve": "1.22.2",
    "resolve-url-loader": "5.0.0",
    "semver": "7.5.2",
    "source-map-loader": "4.0.1",
    "style-loader": "3.3.3",
    "stylelint": "15.6.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard": "33.0.0",
    "stylelint-order": "6.0.3",
    "terser-webpack-plugin": "5.3.9",
    "typescript": "5.0.4",
    "webpack": "5.85.0",
    "webpack-dev-server": "4.15.0",
    "webpack-manifest-plugin": "5.0.0",
    "workbox-webpack-plugin": "7.0.0"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.tsx' && stylelint 'src/**/*.css'",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "test:e2e": "yarn playwright test",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!d3|delaunator|internmap|robust-predicates)"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "browserslist": [
    "last 2 chrome versions",
    "last 2 edge versions",
    "last 2 firefox versions",
    "last 2 safari versions"
  ],
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "packageManager": "yarn@3.5.1"
}
